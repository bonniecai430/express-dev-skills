<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>First Express</title>
</head>
 <body>
   <h1>Skills</h1>
   <ul>
     <% skills.forEach(function(skill) { %>
       <li>
        <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
           - 
         <%= skill.skilled ? 'skilled' : 'not skilled' %>
         <form action="/skills/<%= skill.id %>?_method=DELETE"
          class="delete-form" method="POST">
          <button type="submit">X</button>
        </form>
       </li>
     <% }); %>
   </ul>
   <a href="/skills/new">Add Skill</a>
   <a href="/">Home page</a>
 </body>
</html>